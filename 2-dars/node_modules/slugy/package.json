{
  "name": "slugy",
  "description": "Easily slugify url & titles from any ordinary string.",
  "version": "1.1.0",
  "source": "src/index.ts",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "unpkg": "build/index.umd.js",
  "umd:main": "build/index.umd.js",
  "types": "types/index.d.ts",
  "repository": "git@github.com:Gomah/slugy.git",
  "author": "Gomah <hi@gomah.fr>",
  "license": "MIT",
  "keywords": [
    "slugy",
    "slug",
    "slugify",
    "url",
    "string",
    "utf8",
    "unidecode"
  ],
  "files": [
    "build/*.js"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "unidecode": "^0.1.8"
  },
  "scripts": {
    "build": "rimraf build/ && microbundle --name slugy",
    "dev": "microbundle watch",
    "prepublishOnly": "yarn build",
    "test": "yarn run lint && ava",
    "lint": "eslint --ext .ts src/ types/"
  },
  "devDependencies": {
    "@types/node": "^11.13.9",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "microbundle": "^0.11.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
